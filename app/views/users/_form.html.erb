      <div class="form-label-group">
        <%= form.label :first_name, class:"sr-only" %>
        <%= form.text_field :first_name, type: 'text', class:"form-control", placeholder:"First Name", :required => true, autofocus: true  %>
      </div>

      <div class="text-field">
        <%= form.label :last_name, class:"sr-only" %>
        <%= form.text_field :last_name, type: 'text', class:"form-control", placeholder:"Last Name", :required => true %>
      </div>

      <div class="text-field">
        <%= form.label :email, class:"sr-only" %>
        <%= form.text_field :email, type: 'email', class:"form-control", placeholder:"Email Address", :required => true %>
      </div>

      <div class="password-field">
        <%= form.label :password, class:"sr-only" %>
